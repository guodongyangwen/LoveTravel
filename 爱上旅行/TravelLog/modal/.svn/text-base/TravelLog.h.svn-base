#import <Foundation/Foundation.h>
#import "Author.h"
@interface TravelLog : NSObject
@property(nonatomic,retain)NSString *cover; //背景图片网址
@property(nonatomic,retain)NSString *start_date; //开始日期
@property(nonatomic,retain)NSString *end_data; //结束日期
@property(nonatomic,retain)NSNumber *photo_number; //图片数
@property(nonatomic,retain)NSString *name;
@property(nonatomic,retain)NSString *trail_id;//旅行id（需要传送给详情页面，进行url拼接）
@property(nonatomic,retain)NSString *destination;
@property(nonatomic,retain)NSString *total_days;//总天数
@property(nonatomic,retain)NSString *TravelLogId;//游记ID（需要传送给详情页面）
@property(nonatomic,retain)NSString *avatar;//用户名头像网址
@property(nonatomic,retain)NSString *nickname; //昵称

@property(nonatomic,retain)Author *author; //作者

-(void)dealloc;

@end
