#import "NoCollectView.h"

@implementation NoCollectView

- (id)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        UIImageView *imageV=[[UIImageView alloc]initWithFrame:CGRectMake(20, 5, 70, 100)];
        imageV.image=[UIImage imageNamed:@"img2.png"];
        [self addSubview:imageV];
        [imageV release];
        
        UILabel *labe1=[[UILabel alloc]initWithFrame:CGRectMake(90, 30, 200, 20)];
        labe1.text=@"您还没有自己的收藏";
        [self addSubview:labe1];
        [labe1 release];
        
        UILabel *labe2=[[UILabel alloc]initWithFrame:CGRectMake(120, 65, 120, 20)];
        labe2.text=@"快快添加吧 ~ ~";
        [self addSubview:labe2];
        [labe2 release];
        
    }
    return self;
}

